{
  "name": "blog-con-react-y-vite",
  "version": "1.0.0",
  "description": "Un blog moderno y completo construido con React 19, Vite, Bootstrap 5 y un backend Express.js con MySQL",
  "main": "index.js",
  "scripts": {
    "install:all": "npm install && cd frondend && npm install && cd ../backend && npm install",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frondend && npm run dev",
    "dev:backend": "cd backend && npm start",
    "build": "cd frondend && npm run build",
    "start": "cd backend && npm start",
    "lint": "cd frondend && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charlyanimado/prueba-blog-con-react-y-vite.git"
  },
  "keywords": [
    "react",
    "vite",
    "blog",
    "express",
    "mysql",
    "bootstrap",
    "javascript",
    "frontend",
    "backend",
    "fullstack"
  ],
  "author": "charlyanimado",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/charlyanimado/prueba-blog-con-react-y-vite/issues"
  },
  "homepage": "https://github.com/charlyanimado/prueba-blog-con-react-y-vite#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.0.1",
    "bootstrap": "^5.3.8",
    "prop-types": "^15.8.1",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
